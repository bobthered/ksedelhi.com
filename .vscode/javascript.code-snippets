{
	"Better Default Import": {
		"prefix": "@better default import",
		"body": ["import $2 from '$1';"]
	},
	"Better Named Import": {
		"prefix": "@better named import",
		"body": ["import { $2 } from '$1';"]
	},
	"Svelte Export": {
		"prefix": "svelte-export",
		"body": ["export { default as $1 } from './$1.svelte';"]
	}
}
